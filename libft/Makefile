FLAGS = -Wall -Werror -Wextra
NAME = libft.a
all: $(NAME)
$(NAME):
	@gcc -c $(FLAGS) $(INCS)
	@ar rc $(NAME) $(OBJ)
	@ranlib $(NAME)
clean :
	@rm -f $(OBJ)
fclean : clean
	@rm -f $(NAME)
re : fclean all
